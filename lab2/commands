ls -a|wc -l
time find / -name "README" -type f
who|cut -d ' ' -f 1 | sort | uniq 
nl commands | tee commands2
ls -ld $(pwd)
du -h -d 1 $(find ~ -maxdepth 1 -type d 2>/dev/null) 2>/dev/null
(uname -o; uname -n; uname -m; uname -v)>>sys.info
case $(mesg) in да) echo "Сообщения разрешены"; ;; нет) echo "Сообщения запрещены"; ;; esac

