     1	ls -a|wc -l
     2	time find ~ -name "README" -type f
     3	who|cut -d ' ' -f 1 | sort | uniq 
     4	nl commands | tee commands2
     5	ls -ld $(pwd)
     6	du -h -d 1 $(find ~ -maxdepth 1 -type d 2>/dev/null)
     7	(uname -o; uname -n; uname -m; uname -v)>>sys.info
     8	case $(mesg) in да) echo "Сообщения разрешены"; ;; нет) echo "Сообщения запрещены"; ;; esac
     
